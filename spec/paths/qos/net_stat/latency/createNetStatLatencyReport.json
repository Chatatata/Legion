{
  "tags": [
    "qos",
    "private",
    "net_stat:latency"
  ],
  "summary": "Reports client-side networking latency stats",
  "description": "Creates multiple client-side networking I/O diagnostic entries.",
  "operationId": "createNetStatLatencyReport",
  "parameters": [
    {
      "name": "stats",
      "in": "body",
      "required": true,
      "description": "Contains stats information.",
      "schema": {
        "type": "object",
        "properties": {
          "stats": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "dns_lookup": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Duration of DNS lookup in terms of nanoseconds.",
                  "minimum": 1000
                },
                "tcp_bootstrapping": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Duration of the TCP bootstrapping in terms of nanoseconds.",
                  "minimum": 1000
                },
                "tls_handshake": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Duration of the TLS handshake in terms of nanoseconds.",
                  "minimum": 1000
                },
                "processing": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Processing time of the platform in terms of nanoseconds.",
                  "minimum": 1000
                },
                "content_transfer": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Duration of the content transfer stage in terms of nanoseconds.",
                  "minimum": 1000
                },
                "seconds_since_stealth_auth": {
                  "type": "integer",
                  "format": "int64",
                  "description": "Seconds passed since last successful stealth authentication attempt."
                },
                "connection_type": {
                  "type": "string",
                  "enum": ["wlan", "wwan", "unknown", "none"],
                  "description": "Type of the physical connection."
                },
                "has_vpn": {
                  "type": "boolean",
                  "description": "Indicates the state of VPN."
                }
              }
            }
          }
        }
      },
      "maxLength": 128
    },
  ],
  "responses": {
    "201": {
      "schema": {
        "type": "object",
        "properties": {
          "number_of_entries": {
            "type": "integer",
            "format": "int32",
            "description": "Number of the entries found in request body."
          }
        }
      },
      "description": "Response indicating entries are saved."
    }
  }
}
