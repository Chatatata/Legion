{
  "tags": [
    "user",
    "addresses",
    "mediocre"
  ],
  "summary": "Removes given address of user\n",
  "description": "Deletes given personal address entry of the user with given identifier.\n",
  "operationId": "deleteAddress",
  "parameters": [
    {
      "name": "user_id",
      "in": "path",
      "required": true,
      "description": "Identifier of the platform user.",
      "type": "integer",
      "format": "int64"
    },
    {
      "name": "address_id",
      "in": "path",
      "required": true,
      "description": "Identifier of the address entry.",
      "type": "integer",
      "format": "int64"
    }
  ],
  "responses": {
    "204": {
      "description": "Personal address entry is deleted.\n"
    },
    "404": {
      "description": "The supplied personal address is invalid, the action is rolled back.\n"
    }
  }
}
