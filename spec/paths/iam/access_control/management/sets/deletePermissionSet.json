{
  "tags": [
    "access_control",
    "safe"
  ],
  "summary": "Removes given permission set\n",
  "description": "Deletes given whole permission set (permissions and permission set rows) of the permission set's identifier.\n",
  "operationId": "deletePermissionSet",
  "parameters": [
    {
      "name": "permission_set_id",
      "in": "path",
      "required": true,
      "description": "Identifier of the permission set.",
      "type": "integer",
      "format": "int64"
    }
  ],
  "responses": {
    "204": {
      "description": "Permission set is deleted.\n"
    },
    "404": {
      "description": "The supplied permission set is invalid, the action is rolled back.\n"
    }
  }
}
